    </fragment>
    <fragment
        android:id="@+id/c"
        android:name="com.arsa.trackerapp.view.fragments.C"
        android:label="fragment_c"
        tools:layout="@layout/fragment_c" >
        <action
            android:id="@+id/action_c_to_d"
            app:destination="@id/d" />
    </fragment>
    <fragment
        android:id="@+id/e"
        android:name="com.arsa.trackerapp.view.fragments.E"
        android:label="fragment_e"
        tools:layout="@layout/fragment_e" >
        <action
            android:id="@+id/action_e_to_a"
            app:destination="@id/a" />
    </fragment>
    <fragment
        android:id="@+id/d"
        android:name="com.arsa.trackerapp.view.fragments.D"
        android:label="fragment_d"
        tools:layout="@layout/fragment_d" >
        <action
            android:id="@+id/action_d_to_e"
            app:destination="@id/e" />
    </fragment>
</navigation>